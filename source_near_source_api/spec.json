{
  "documentationUrl": "https://cuthanh.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Near sync transaction",
    "type": "object",
    "required": ["start_block", "figment_key"],
    "additionalProperties": false,
    "properties": {
      "start_block": {
        "type": "string",
        "description": "Start of block height",
        "examples": ["0"]
      },
      "figment_key": {
        "type": "string",
        "examples": [""],
        "description": "Figment key at https://figment.io/"
      }
    }
  }
}
